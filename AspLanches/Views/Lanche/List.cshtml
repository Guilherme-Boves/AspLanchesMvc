@model LancheListViewModel

@{
    ViewData["Title"] = "Todos os lanches";
}

<div> <h2>@ViewData["Title"]</h2></div>
<hr />
<h3>@ViewBag.Total @ViewBag.TotalLanches</h3>

@*<h3>@TempData["Nome"]</h3>*@

<div class="container">
    <div class="row">
        @foreach (var lanche in Model.Lanches)
        {
           <partial name="_LanchesResumo" for="@lanche" />
        }
    </div>
</div>