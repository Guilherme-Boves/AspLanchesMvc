@model IEnumerable<AspLanches.Models.Lanche>

@{
    ViewData["Title"] = "Todos os lanches";
}

<div> <h2>@ViewData["Title"]</h2></div>

@foreach(var lanche in Model)
{
    <div>
        <h4>@lanche.Nome</h4>
        <p><img src="@lanche.ImagemUrl"/></p>
        <h3>Preço: @lanche.Preco.ToString("c")</h3>
    </div>
}