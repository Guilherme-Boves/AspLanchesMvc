@model Lanche

<div class="card col-sm-4 col-md-4 col-lg-4" style="width: 18rem;margin-right: 30px">
    <img src="@Model.ImagemUrl" class="card-img-top" alt="@Model.Nome - Img">
    <div class="card-body">
        <h5 class="card-title">@Model.Nome</h5>
        <h6 class="card-subtitle mb-2 text-muted">R$ @Model.Preco</h6>
        <p class="card-text">@Model.DescricaoCurta</p>
    </div>
</div>     